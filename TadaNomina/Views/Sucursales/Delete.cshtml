@model TadaNomina.Models.ViewModels.Catalogos.ModelSucursales

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal"><i class="pci-cross pci-circle"></i></button>
    <h4 class="modal-title">Eliminar Sucursal</h4>
</div>
<div class="modal-body">

    <label> Esta seguro que desea eliminar la siguiente Sucursal?</label>
    <br />
    <dl class="dl-horizontal">
        <dt>


            @Html.DisplayNameFor(model => model.Clave)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Clave)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Sucursal)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Sucursal)
        </dd>

    </dl>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="modal-footer">
            <div class="form-actions no-color">
                <button class="btn btn-default btn-sm" data-dismiss="modal" type="button">Cancelar</button>
                <input type="submit" value="Eliminar" class="btn btn-danger btn-sm" />
            </div>
        </div>
    }

</div>

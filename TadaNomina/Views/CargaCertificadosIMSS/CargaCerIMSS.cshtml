@model TadaNomina.Models.ViewModels.MovimientosIMSS.mCredencialesMovimientosIMSS
@{
    ViewBag.Title = "CargaCerIMSS";
    Layout = "~/Views/Shared/_layoutPrincipal.cshtml";
}

<div id="page-head">
    <div id="page-title">
        <h1 class="page-header text-overflow">Registro Patronal</h1>
    </div>
    <div id="rutaPrincipal"></div>
</div>


<div class="col-md-8 col-md-offset-2">
    <div id="page-content ">
        <div class="panel">
            <div class="panel-heading">
                <h3 class="panel-title">Carga de certificado(<span class="text-muted">Certificado mivimientos</span>)</h3>
            </div>

            <div class="panel-body">


                @using (Html.BeginForm("CargaCerIMSS", "CargaCertificadosIMSS", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    @Html.AntiForgeryToken()

                    <div class="form-horizontal">

                        <hr />
                        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

                        @Html.HiddenFor(model => model.IdReg)

                        <div class="form-group">
                            @Html.LabelFor(model => model.Archivo, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                @Html.TextBoxFor(model => model.Archivo, new { type = "file", @class = "inputfile" })
                                @Html.ValidationMessageFor(model => model.Archivo, "", new { @class = "text-danger" })
                            </div>
                        </div>

                        <div class="form-group">
                            @Html.LabelFor(model => model.Key, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                @Html.TextBoxFor(model => model.Key, new { type = "file", @class = "inputfile" })
                                @Html.ValidationMessageFor(model => model.Key, "", new { @class = "text-danger" })
                            </div>
                        </div>

                        <div class="form-group">
                            @Html.LabelFor(model => model.UsuarioIMSS, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                @Html.EditorFor(model => model.UsuarioIMSS, new { htmlAttributes = new { @class = "form-control", autocomplete = "new-password" } })
                                @Html.ValidationMessageFor(model => model.UsuarioIMSS, "", new { @class = "text-danger" })
                            </div>
                        </div>
                        <div class="form-group">
                            @Html.LabelFor(model => model.ContrasenaIMSS, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                @Html.EditorFor(model => model.ContrasenaIMSS, new { htmlAttributes = new { @class = "form-control", autocomplete = "new-password" } })
                                @Html.ValidationMessageFor(model => model.ContrasenaIMSS, "", new { @class = "text-danger" })
                            </div>
                        </div>
                        <br />
                        <div class="form-group">
                            <div class="text-right">

                                <button class="btn btn-primary" type="submit">Cargar</button>
                                @Html.ActionLink("Regresar", "Index", null, new { @class = "btn btn-info _Back" })
                            </div>
                        </div>
                    </div>
                }

            </div>
        </div>
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}


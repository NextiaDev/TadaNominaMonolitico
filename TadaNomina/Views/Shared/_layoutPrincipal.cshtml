                        else
                        {
                            <div>
                                Cliente y Nómina: <a href="@Url.Action("Index", "Default")" class="text-main" onclick="borraLocalStorage()"><span class="badge badge-mint">@ViewBag.ClienteSeleccionado</span></a> &nbsp;
                                -&nbsp; <a href="@Url.Action("Index", "Default")" class="text-main" onclick="borraLocalStorage()"><span class="badge badge-mint">@ViewBag.NominaSeleccionada</span></a>
                            </div>

                        }
                    }
                </div>

                <!-- Visible when footer positions are static -->
